  <servlet>
    <servlet-name>JerseyServlet</servlet-name>
    <servlet-class>com.sun.jersey.spi.container.servlet.ServletContainer</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>JerseyServlet</servlet-name>
    <url-pattern>/resources/*</url-pattern>
  </servlet-mapping>
   
  <servlet>
    <description/>
    <servlet-name>ProductViewServlet</servlet-name>
    <servlet-class>org.jcandystore.views.ProductViewServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ProductViewServlet</servlet-name>
    <url-pattern>/productView</url-pattern>
  </servlet-mapping>

  <servlet>
    <description/>
    <servlet-name>ShoppingCartServlet</servlet-name>
    <servlet-class>org.jcandystore.views.ShoppingCartServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ShoppingCartServlet</servlet-name>
    <url-pattern>/cart</url-pattern>
  </servlet-mapping>

  <servlet>
    <description/>
    <servlet-name>CheckoutServlet</servlet-name>
    <servlet-class>org.jcandystore.views.CheckoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CheckoutServlet</servlet-name>
    <url-pattern>/checkout</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>SystemServiceServlet</servlet-name>
    <servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
    <init-param>
      <param-name>services</param-name>
      <param-value>org.jcandystore.endpoints.AccountEndpoint,org.jcandystore.endpoints.ItemEndpoint,org.jcandystore.endpoints.OrdersEndpoint,org.jcandystore.endpoints.ProductEndpoint</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>SystemServiceServlet</servlet-name>
    <url-pattern>/_ah/spi/*</url-pattern>
  </servlet-mapping>
</web-app>